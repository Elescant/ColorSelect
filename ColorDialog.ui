   <string notr="true">/*主界面样式表*/
QWidget
{
	border:none;
	color:white;
	font-family:微软雅黑;
	font-size:14px;
	outline:0px;
	background:transparent;
}

#topWgt
{
	border-image: url(:/Image/data/images/mainui/bg.png) 4 4 4 4 stretch stretch;
	border-width:4 4 4 4;
}

/*
主体背景
*/
#mainWgt
{
	border-image: url(:/Image/data/images/mainui/main.png) 4 4 4 4 stretch stretch;
	border-width:4 4 4 4;
}
QLineEdit
{
	background:#25272b;
	border:1px solid #276d94;
	padding-left:5px;
}

QGroupBox
{
	border:2px solid #276d94;
	margin-top:10px;
}
QGroupBox::title
{
	left:10px;
	bottom:10px;
}

/*1-按钮开始*/
QPushButton
{
	background:#b82525;
	border-radius:3px;
	padding-top:-2px;
}
QPushButton:hover
{
	background:#d4352d;
}
QPushButton:pressed
{
	background:rgb(136, 138, 133);
}
QPushButton::disabled
{
	background:rgb(136, 138, 133);
}
/*1-按钮结束*/
QSpinBox::up-button {
    subcontrol-origin: border;
    subcontrol-position:right; /* position at the top right corner */
	height:27px;
    width: 24px; /* 16 + 2*1px border-width = 15px padding + 3px parent border */
    background-color:#25272b;
    border-width: 1px;
}

QSpinBox::up-arrow {
	image: url(:/Image/data/images/button/arrow_right.png);
   height:  27px;
	width: 24px;
    
}

QSpinBox::down-button {
    subcontrol-origin: border;
    subcontrol-position: left; /* position at bottom right corner */
    height:27px;
	width: 24px;	 
    border-width: 1px;
	 background-color:#25272b;
    border-top-width: 0;
}

QSpinBox::down-arrow {
    image:url(:/Image/data/images/button/arrow_left.png);
    height: 27px; 
	width: 24px;
}


QSpinBox
{
    /*padding-right: 15px;  make room for the arrows */
    border-width: 1;

	border:1px solid #276d94;   
}
